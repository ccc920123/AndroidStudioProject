<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">

        <FrameLayout
            android:id="@+id/framelayout"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="1.0"
            android:orientation="vertical">

            <com.znsd.water.waterapp.bannerview.ViewFlow
                android:id="@+id/viewflow"
                android:layout_width="fill_parent"
                android:layout_height="150dp"
                android:background="@color/white" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:background="@color/transparent"
                android:gravity="center"
                android:padding="3dip">

                <com.znsd.water.waterapp.bannerview.CircleFlowIndicator
                    android:id="@+id/viewflowindic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal|bottom"
                    android:padding="2dip"
                    app:activeColor="#ff0000"
                    app:activeType="fill"
                    app:circleSeparation="20dip"
                    app:inactiveColor="#ffffff"
                    app:inactiveType="fill"
                    app:radius="4dip" />
            </FrameLayout>
        </FrameLayout>

        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5.0sp"
            android:layout_marginRight="5.0sp"
            android:layout_marginTop="10.0sp"
            android:background="#ffffff"
            android:stretchColumns="1">

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:gravity="left"
                    android:text="@string/cy"
                    android:textColor="#ff000000" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg">

                <TextView
                    android:id="@+id/zhf"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:layout_weight="1.0"
                    android:background="@drawable/selector_btn"
                    android:drawableTop="@mipmap/zhf"
                    android:gravity="center"
                    android:padding="6.0dip"
                    android:text="综合费"
                    android:textColor="#ff000000" />

                <TextView
                    android:id="@+id/sf"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:layout_weight="1.0"
                    android:background="@drawable/selector_btn"
                    android:drawableTop="@mipmap/sf"
                    android:gravity="center"
                    android:text="水费"
                    android:textColor="#ff000000" />

                <TextView
                    android:id="@+id/df"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:layout_weight="1.0"
                    android:background="#ffffffff"
                    android:drawableTop="@mipmap/df"
                    android:gravity="center"
                    android:padding="6.0dip"
                    android:text="电费"
                    android:textColor="#ff000000" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg">

                <TextView
                    android:id="@+id/rqf"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:layout_weight="1.0"
                    android:background="#ffffffff"
                    android:drawableTop="@mipmap/qf"
                    android:gravity="center"
                    android:padding="6.0dip"
                    android:text="燃气费"
                    android:textColor="#ff000000" />

                <TextView
                    android:id="@+id/lqf"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:layout_weight="1.0"
                    android:background="#ffffffff"
                    android:drawableTop="@mipmap/lq"
                    android:gravity="center"
                    android:text="暖气费"
                    android:textColor="#ff000000" />

                <TextView
                    android:id="@+id/wyf"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:layout_weight="1.0"
                    android:background="#ffffffff"
                    android:drawableTop="@mipmap/wy"
                    android:gravity="center"
                    android:padding="6.0dip"
                    android:text="物业费"
                    android:textColor="#ff000000" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:gravity="left"
                    android:text="@string/zb"
                    android:textColor="#ff000000" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg">

                <TextView
                    android:id="@+id/ds"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:layout_weight="1.0"
                    android:background="#ffffffff"
                    android:drawableTop="@mipmap/ds"
                    android:gravity="center"
                    android:padding="6.0dip"
                    android:text="订水"
                    android:textColor="#ff000000" />

                <TextView
                    android:id="@+id/sg"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:layout_weight="1.0"
                    android:background="#ffffffff"
                    android:drawableTop="@mipmap/sg"
                    android:gravity="center"
                    android:text="水果"
                    android:textColor="#ff000000" />

                <TextView
                    android:id="@+id/js"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:layout_weight="1.0"
                    android:background="#ffffffff"
                    android:drawableTop="@mipmap/js"
                    android:gravity="center"
                    android:padding="6.0dip"
                    android:text="酒水"
                    android:textColor="#ff000000" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg">

                <TextView
                    android:id="@+id/xn"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:layout_weight="1.0"
                    android:background="#ffffffff"
                    android:drawableTop="@mipmap/xn"
                    android:gravity="center"
                    android:padding="6.0dip"
                    android:text="鲜奶"
                    android:textColor="#ff000000" />

                <TextView
                    android:id="@+id/xh"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:layout_weight="1.0"
                    android:background="#ffffffff"
                    android:drawableTop="@mipmap/xh"
                    android:gravity="center"
                    android:text="鲜花"
                    android:textColor="#ff000000" />

                <TextView
                    android:id="@+id/more"
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginLeft="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:layout_weight="1.0"
                    android:background="#ffffffff"
                    android:drawableTop="@mipmap/gd"
                    android:gravity="center"
                    android:padding="6.0dip"
                    android:text="更多"
                    android:textColor="#ff000000" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:gravity="left"
                    android:text="见闻"
                    android:textColor="#ff000000" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg">

                <TextView
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="1.0dip"
                    android:layout_marginTop="1.0dip"
                    android:layout_weight="1.0"
                    android:background="#ffffffff"
                    android:gravity="center"
                    android:padding="6.0dip"
                    android:text="今日头条"
                    android:textColor="#ff000000" />
            </TableRow>

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="0.0dip"
                    android:layout_height="fill_parent"
                    android:layout_weight="1.0"
                    android:gravity="left"
                    android:text="热门"
                    android:textColor="#ff000000" />
            </TableRow>
        </TableLayout>

        <FrameLayout
            android:id="@+id/framelayout1"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_weight="1.0"
            android:orientation="vertical">

            <com.znsd.water.waterapp.bannerview.ViewFlow
                android:id="@+id/viewflow1"
                android:layout_width="fill_parent"
                android:layout_height="150dp"
                android:background="@color/white" />

            <FrameLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top"
                android:background="@color/transparent"
                android:gravity="center"
                android:padding="3dip">

                <com.znsd.water.waterapp.bannerview.CircleFlowIndicator
                    android:id="@+id/viewflowindic1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal|top"
                    android:padding="2dip"
                    app:activeColor="#ff0000"
                    app:activeType="fill"
                    app:circleSeparation="20dip"
                    app:inactiveColor="#ffffff"
                    app:inactiveType="fill"
                    app:radius="4dip" />
            </FrameLayout>
        </FrameLayout>


    </LinearLayout>
</ScrollView>